<%= form_for(@user) do |f| %>

  <div class="form-group">
    <%= f.label :user_name, "ユーザー名"%><br>
    <%= f.text_field :user_name, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :email, "電子メール"%><br>
    <%= f.text_field :email, class: "form-control" %>
  </div>

  <%= f.submit "登録する", class: "btn btn-default" %>

<% end %>
